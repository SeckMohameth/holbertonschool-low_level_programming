#include "holberton.h"
#include <stdlib.h>
#include <stdio.h>

char *rot13(char *)
{
	int x, y;

	char arr[] = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";

	char arr2[] = "nopqrstuvwxyzabcdefghijklm";

	for (x = 0; arr[x]; x++)
	{
		for (y = 0; arr2[y]; y++)
		{
			if (arr[x] == arr[y])
			{
				arr[x] == arr2[y];
					break;
			}
		}
	}

	return (0);
}
